{
  "name": "object-transformer",
  "version": "0.0.5",
  "repository": "https://github.com/manuelvulp/object-transformer",
  "homepage": "https://github.com/manuelvulp/object-transformer",
  "description": "Javascript object transformer",
  "main": "index",
  "keywords": [
    "node",
    "object",
    "transform",
    "transformer",
    "parse",
    "parser",
    "format",
    "formatter",
    "schema",
    "model",
    "database"
  ],
  "directories": {
    "example": "example"
  },
  "author": "Manuel Vulp",
  "license": "MIT",
  "devDependencies": {
    "blanket": "^1.1.6",
    "coveralls": "^2.10.0",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-env": "^0.4.1",
    "grunt-mocha-istanbul": "^1.4.1",
    "grunt-mocha-test": "^0.11.0",
    "grunt-shell": "^0.7.0",
    "mocha": "~1.19.0",
    "mocha-lcov-reporter": "0.0.1",
    "must": "~0.11.0",
    "require-dir": "^0.1.0",
    "sinon": "~1.10.0"
  },
  "scripts": {
    "test": "mocha  --require must --recursive -c --reporter spec",
    "testw": "mocha  --require must --recursive -c --reporter spec -w",
    "coverage": "mocha --require blanket --require must --reporter html-cov test/**/*.js > coverage.html",
    "coveralls": "mocha --require blanket --require must --reporter mocha-lcov-reporter test/**/*.js | coveralls"
  },
  "data-cover-never": [
    "node_modules",
    "test"
  ],
  "config": {
    "blanket": {
      "pattern": [
        "src/"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "dependencies": {
    "babel-core": "^5.8.25",
    "json-query": "^1.5.0"
  }
}
